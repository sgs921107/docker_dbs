COMPOSE_PROJECT_NAME=dbs
# 宿主机中的日志目录
HOST_LOG_DIR=/var/log/dbs
# 宿主机中的项目目录
HOST_PROJECT_DIR=/app/docker_dbs

# 启动的服务 1(启动)/其它(不启动)
SERVICE_MYSQL=1
SERVICE_REDIS=1
SERVICE_ES=1
SERVICE_ES_HEAD=1
SERVICE_MONGO=1
SERVICE_RABBITMQ=1

# mysql 版本
MYSQL_VERSION=8.0
# mysql配置文件在宿主机中的地址
HOST_MYSQL_CONF=/etc/dbs/my.cnf
# mysql服务在宿主机中的端口
HOST_MYSQL_PORT=3306
# root密码
MYSQL_ROOT_PASSWORD=123456
# 允许访问登录root账户的主机
MYSQL_ROOT_HOST=localhost

# redis版本
REDIS_VERSION=7.4
# redis配置文件在宿主机中的地址
HOST_REDIS_CONF=/etc/dbs/redis.conf
# redis服务在宿主机中的端口
HOST_REDIS_PORT=6379

# es版本
ES_VERSION=6.6.2
# es配置文件在宿主机中的地址
HOST_ES_CONF=/etc/dbs/elasticsearch.yml
# es服务在宿主机中的tcp端口
HOST_ES_TCP_PORT=9300
# es服务在宿主机中的http端口
HOST_ES_HTTP_PORT=9200

# es_head版本
ES_HEAD_VERSION=5
# es head服务在宿主机中的端口
HOST_ES_HEAD_PORT=9100

# mongo版本
MONGO_VERSION=4.4.5
MONGO_ROOT_USERNAME=admin
MONGO_ROOT_PASSWORD=123456
# mongo服务在宿主机中的端口
HOST_MONGO_PORT=27017

# rabbit版本
RABBITMQ_VERSION=3.8.16-management
# rabbit服务在宿主机中的端口
HOST_RABBITMQ_PORT=5672
# rabbit后台服务在宿主机中的端口
HOST_RABBITMQ_ADMIN_PORT=15672
RABBITMQ_DEFAULT_USER=work
RABBITMQ_DEFAULT_PASS=online
RABBITMQ_ERLANG_COOKIE=VeryComplicated
